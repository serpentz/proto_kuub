{
  "name": "proto_kuub",
  "version": "1.0.0",
  "type": "module",
  "description": "Prototype of Kuub backend, for minimal services",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "reset:db:sqlite": "sh ./tools/reset_db_sqlite.sh",
    "reset:db:pg": "sh ./tools/reset_db_pg.sh",
    "babel": "babel --presets es2016 ./server.js -o build/main.bundle.js",
    "compile": "babel . --out-dir .compiled --source-maps --watch",
    "build": "babel ./server.js -d dist",
    "start:babel": "node dist/server.js",
    "dev": "node ./server.js"
  },
  "author": "Absalom",
  "license": "ISC",
  "dependencies": {
    "@apollo/server": "^4.9.2",
    "graphql": "^16.8.0",
    "mysql2": "^3.6.0",
    "pg": "^8.11.3",
    "sequelize": "^6.32.1",
    "sqlite3": "^5.1.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.10",
    "@babel/core": "^7.22.11",
    "@babel/preset-env": "^7.22.10",
    "babel-cli": "^6.26.0",
    "babel-plugin-import-directory": "^1.1.0",
    "babel-preset-es2015": "^6.24.1"
  }
}
